ring R=0,(t,x,y,z),lp;
poly f1=t^2-3;
poly f2=x^2+x+1;
poly f3=y^3-(1/2);
poly f4=z-t-x*y;
ideal I=f1,f2,f3,f4;
ideal G=groebner(I);
G;
